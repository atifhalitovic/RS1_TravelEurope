<!DOCTYPE html>
@using Microsoft.AspNetCore.Identity
@using System.Security.Claims;
@inject SignInManager<ApplicationUser> signInManager
@inject UserManager<ApplicationUser> userManager

<html>
<head>
    <meta charset="utf-8">
    <title>TravelEurope</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">
    <environment include="Production">
        <!-- Favicons -->
        <link href="img/favicon.png" rel="icon" asp-append-version="true">
        <link href="img/apple-touch-icon.png" rel="apple-touch-icon" asp-append-version="true">
        <!-- Google Fonts -->
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Raleway:300,400,500,700,800|Montserrat:300,400,700" rel="stylesheet" asp-append-version="true">
        <!-- Bootstrap CSS File -->
        <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true">
        <!-- Libraries CSS Files -->
        <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css" asp-append-version="true">
        <link href="vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" asp-append-version="true">
        <link href="vendor/animate/animate.min.css" rel="stylesheet" asp-append-version="true">
        <link href="vendor/ionicons/css/ionicons.min.css" rel="stylesheet" asp-append-version="true">
        <link href="vendor/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet" asp-append-version="true">
        <link href="vendor/magnific-popup/magnific-popup.css" rel="stylesheet" asp-append-version="true">
        <link href="vendor/ionicons/css/ionicons.min.css" rel="stylesheet" asp-append-version="true">
        <!-- Main Stylesheet File -->
        <link href="css/style.css" rel="stylesheet" asp-append-version="true">
    </environment>
    <title>TravelEurope system</title>
</head>
<body id="body">
    <!--==========================
      Top Bar
    ============================-->
    <section id="topbar" class="d-none d-lg-block">
        <div class="container clearfix">
            <div class="contact-info float-left">
                <i class="fa fa-envelope-o"></i> <a href="mailto:kontakt@traveleurope.ba">kontakt@traveleurope.ba</a>
                <i class="fa fa-phone"></i> +387 37 400 000
            </div>
            <div class="social-links float-right">
                <a href="#" class="twitter"><i class="fa fa-twitter"></i></a>
                <a href="#" class="facebook"><i class="fa fa-facebook"></i></a>
                <a href="#" class="instagram"><i class="fa fa-instagram"></i></a>
                <a href="#" class="google-plus"><i class="fa fa-google-plus"></i></a>
                <a href="#" class="linkedin"><i class="fa fa-linkedin"></i></a>
            </div>
        </div>
    </section>
    <!--==========================
      Header
    ============================-->
    <header id="header">
        <div class="container">
            <div id="logo" class="pull-left">
                <h1><a href="#body" class="scrollto">Travel<span>Europe</span></a></h1>
            </div>
            <nav id="nav-menu-container">
                @if (signInManager.IsSignedIn(User) && (User.IsInRole("Klijent")))
                {
                <ul class="nav-menu">
                    <li class="menu-active"><a href="#">Home</a></li>
                    <li><a href="#about">O nama</a></li>
                    <li><a href="#services">Ponude</a></li>
                    <li><a href="#portfolio">Portfolio</a></li>
                    <li><a href="#team">Naš tim</a></li>
                    <li><a href="#contact">Kontakt</a></li>
                    <li class="nav-item dropdown no-arrow">
                        <i class="fas fa-user-circle fa-fw"></i>
                        <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown"
                           aria-haspopup="true" aria-expanded="false">Dobrodošao, @User.Identity.Name    </a>
                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                            <a class="dropdown-item" asp-controller="Profil" asp-action="Detalji" asp-route-id="@this.User.FindFirstValue(ClaimTypes.NameIdentifier)">Detalji</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" asp-controller="Account" asp-action="Logout">Logout</a>
                        </div>
                    </li>
                </ul>
                }
                else if (signInManager.IsSignedIn(User) && (User.IsInRole("Administrator")))
                {
                    <ul class="nav-menu">
                        <li class="menu-active"><a href="#">Home</a></li>
                        <li><a href="#about">O nama</a></li>
                        <li><a href="#services">Ponude</a></li>
                        <li><a href="#portfolio">Portfolio</a></li>
                        <li><a href="#team">Naš tim</a></li>
                        <li><a href="#contact">Kontakt</a></li>
                        <li class="nav-item dropdown no-arrow">
                            <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown"
                               aria-haspopup="true" aria-expanded="false">
                                Dobrodošao, @User.Identity.Name
                            </a><i class="fas fa-user-circle fa-fw"></i>
                            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                                <a class="dropdown-item" asp-area="AdminPanel" asp-controller="Home">Radni panel</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" asp-controller="Account" asp-action="Logout">Logout</a>
                            </div>
                        </li>
                    </ul>
                }
                else if (signInManager.IsSignedIn(User) && (User.IsInRole("Radnik")))
                {
                    <ul class="nav-menu">
                        <li class="menu-active"><a href="#">Home</a></li>
                        <li><a href="#about">O nama</a></li>
                        <li><a href="#services">Ponude</a></li>
                        <li><a href="#portfolio">Portfolio</a></li>
                        <li><a href="#team">Naš tim</a></li>
                        <li><a href="#contact">Kontakt</a></li>
                        <li class="nav-item dropdown no-arrow">
                            <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown"
                               aria-haspopup="true" aria-expanded="false">
                                Dobrodošao, @User.Identity.Name
                            </a><i class="fas fa-user-circle fa-fw"></i>
                            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                                <a class="dropdown-item" asp-area="RadnikPanel" asp-controller="Home">Radni panel</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" asp-controller="Account" asp-action="Logout">Logout</a>
                            </div>
                        </li>
                    </ul>
                }
                else
                {
                    <ul class="nav-menu">
                        <li class="menu-active"><a href="#">Home</a></li>
                        <li><a href="#about">O nama</a></li>
                        <li><a href="#services">Ponude</a></li>
                        <li><a href="#portfolio">Portfolio</a></li>
                        <li><a href="#team">Naš tim</a></li>
                        <li><a href="#contact">Kontakt</a></li>
                        <li><a asp-controller="Profil" asp-action="Kreiraj">Registracija</a></li>
                        <li class="nav-item dropdown no-arrow">
                            <i class="fas fa-user-circle fa-fw"></i>
                            <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown"aria-haspopup="true" aria-expanded="false"></a>
                            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" asp-controller="Account" asp-action="Login">Login</a>
                            </div>
                        </li>
                    </ul>
                }
            </nav>
            <!-- #nav-menu-container -->
        </div>
    </header><!-- #header -->
    <!--==========================
      Intro Section
    ============================-->
    <section id="intro">
        <div class="intro-content">
            <h2>Doživite <span>putovanje života!</span><br><span>Sa nama</span> - moguće!</h2>
            <div>
                <a href="#about" class="btn-get-started scrollto">Više o nama</a>
                <a href="#portfolio" class="btn-projects scrollto">Naša ponuda</a>
            </div>
        </div>
        <div id="intro-carousel" class="owl-carousel">
            <div class="item" style="background-image: url('img/intro-carousel/1.jpg');"></div>
            <div class="item" style="background-image: url('img/intro-carousel/2.jpg');"></div>
            <div class="item" style="background-image: url('img/intro-carousel/3.jpg');"></div>
            <div class="item" style="background-image: url('img/intro-carousel/4.jpg');"></div>
            <div class="item" style="background-image: url('img/intro-carousel/5.jpg');"></div>
        </div>
    </section><!-- #intro -->

    <main id="main">
        <!--==========================
          About Section
        ============================-->
        <section id="about" class="wow fadeInUp">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 about-img">
                        <img src="img/about-img.jpg" alt="">
                    </div>
                    <div class="col-lg-6 content">
                        <h2>Ponuda najsavremenijih vozila, prilagođenih turist ruta i profesionalnih vozača!</h2>
                        <h3>Certificirani od strane međunarodne organizacije za rent-a-car i turističke agencije.</h3>
                        <ul>
                            <li><i class="ion-android-checkmark-circle"></i> Savremena motorna vozila</li>
                            <li><i class="ion-android-checkmark-circle"></i> Prilagođene turist rute za sve uzraste sa profesionalnim turističkim vodičem</li>
                            <li><i class="ion-android-checkmark-circle"></i> Profesionalni certificani vozači sa dugogodišnjim iskustvom Vama na usluzi</li>
                        </ul>
                    </div>
                </div>
            </div>
        </section><!-- #about -->
        <partial name="_CookieConsentPartial" />
        <section id="services">
            <div class="container">
                <div class="section-header">
                    <h2>Osnovna ponuda</h2>
                    <p>Pogledajte našu cjelovitu ponudu i izaberite ono šta vama najviše odgovara.</p>
                    <br /><p>@RenderBody()</p>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="box wow fadeInLeft">
                            <div class="icon"><i class="fa fa-car"></i></div>
                            <h4 class="title"><a href="#portfolio">Motorna vozila</a></h4>
                            <p class="description">Iznajmite savremeno vozilo iz naše ponude za vaš ukus. Automobil, skuter, motor - čekaju na vas.</p>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="box wow fadeInRight">
                            <div class="icon"><i class="fa fa-toggle-off"></i></div>
                            <h4 class="title"><a>Turist rute</a></h4>
                            <p class="description">Izaberite jednu turist rutu iz naše ponude prilagođenu vašim zahtjevima i propozicijama.</p>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="box wow fadeInLeft" data-wow-delay="0.2s">
                            <div class="icon"><i class="fa fa-car-side"></i></div>
                            <h4 class="title"><a>Privatni vozači</a></h4>
                            <p class="description">Ekipa profesionalnih vozača čeka vas da izaberete i počnete vašu vožnju samo uz TravelEurope.</p>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="box wow fadeInRight" data-wow-delay="0.2s">
                            <div class="icon"><i class="fa fa-map"></i></div>
                            <h4 class="title"><a>Osiguranje</a></h4>
                            <p class="description">Budite osigurani tokom cijelog putovanja. Izaberite osiguranje po vašoj mjeri, samo u TravelEurope.</p>
                        </div>
                    </div>
                </div>
            </div>
        </section><!-- #services -->

        <section id="portfolio" class="wow fadeInUp">
            <div class="container">
                <div class="section-header">
                    <h2>Portfolio</h2>
                    <p>Na idućim fotografijama možete vidjeti neka vozila i turist rute iz naše ponude.</p>
